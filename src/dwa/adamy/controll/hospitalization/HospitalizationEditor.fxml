<?xml version="1.0" encoding="UTF-8"?>

<?import dwa.adamy.controll.patient.PatientFinder?>
<?import dwa.adamy.ui.prop.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.121"
         xmlns:fx="http://javafx.com/fxml/1">

    <HospitalizationUnitProp fx:id="hospitalizationUnitProp"
                             label="Jednostka hospitalizacji"/>

    <DateProp fx:id="fromDateProp" label="Od daty:"/>
    <TimeProp fx:id="fromTimeProp" label="Od godziny:"/>

    <DateProp fx:id="toDateProp" label="Do daty:"/>
    <TimeProp fx:id="toTimeProp" label="Do godziny:"/>

    <PatientFinder fx:id="patientFinder"/>

    <HBox alignment="TOP_RIGHT">
        <Button onAction="#cancelAction" text="Anuluj"/>
        <Button onAction="#saveAction" text="Zapisz"/>
    </HBox>


</fx:root>
